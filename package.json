{
  "name": "@sandiz/psarcjs",
  "version": "0.1.5",
  "description": "node module to read and extract Rocksmith PSARC files",
  "main": "dist/index.js",
  "scripts": {
    "test": "python3 test/sng/generate-sng-json.py && mocha --reporter spec",
    "drypack": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "cover": "c8 --reporter=html --reporter=text mocha ./test/test.js",
    "build": "tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandiz/psarcjs.git"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "keywords": [
    "psarc",
    "psarcjs",
    "rocksmith",
    "rocksmith2014"
  ],
  "author": "Sandi Chakravarty <sandi.newton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandiz/psarcjs/issues"
  },
  "homepage": "https://github.com/sandiz/psarcjs#readme",
  "dependencies": {
    "aes-js": "^3.1.2",
    "binary-parser": "^1.5.0"
  },
  "devDependencies": {
    "@types/aes-js": "^3.1.0",
    "@types/binary-parser": "^1.3.1",
    "@types/node": "^12.12.17",
    "c8": "^5.0.4",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "chai-fs": "^2.0.0",
    "coveralls": "^3.0.9",
    "mocha": "^6.2.2",
    "mocha-each": "^2.0.1",
    "tmp": "^0.1.0",
    "typescript": "^3.7.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
